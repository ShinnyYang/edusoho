{% extends 'layout.html.twig' %}

{% do script(['libs/vue.js', 'app/js/answer-engine/report/index.js']) %}

{% block body %}
  <style>
    html, body {
      -webkit-overflow-scrolling: touch;
    }
  </style>
{% include 'answer-engine/common.html.twig' %}
{% include 'attachment-upload/upload-data.html.twig' %}
<input type="hidden" name="answer_record_id" value='{{ answerRecordId }}'>
<input type="hidden" name="restart_url" value='{{ restartUrl }}'>
<input type="hidden" name="return_url" value="{{ returnUrl }}">
<input type="hidden" name="answer_show" value='{{ answerShow }}'>
<input type="hidden" name="collect" value='{{ collect }}'>
<input type="hidden" name="submit_return_url" value="{{ submitReturnUrl|default('') }}">
<input type="hidden" name="show_do_again_btn" value="{{ showDoAgainBtn|default(1) }}">
<input type="hidden" name="assessment_status" value="{{ assessmentStatus }}">
  {% if is_answer_show_ai_agent(answerRecordId) %}
    <input type="hidden" id="aiAgentToken" value="{{ ai_agent_token() }}" />
    <input type="hidden" id="chatMetaData" value="{{ get_answer_chat_meta_data(answerRecordId) }}" />
    <input type="hidden" id="studyPlanGenerated" value="1" />
  {% endif %}
{% endblock %}

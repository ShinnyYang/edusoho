{% import 'admin-v2/macro.html.twig' as admin_macro %}
<tr id="user-table-tr-{{ smsBlackIp.id }}">
  <td>
    <span>{{ smsBlackIp.ip }}</span>
  </td>
  <td>
    <span class="text-sm">{{ smsBlackIp.created_time|date('Y-n-d H:i:s') }}</span>
  </td>
  <td>
    <span class="text-sm">{{ smsBlackIp.expire_time|date('Y-n-d H:i:s') }}</span>

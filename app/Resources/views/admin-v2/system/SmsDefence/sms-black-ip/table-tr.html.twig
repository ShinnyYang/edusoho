  <td>
    <a href="#modal" data-toggle="modal" data-url="{{ path('admin_v2_login_record_details', {id:smsBlackIp.id}) }}" data-url="" class="btn btn-default btn-sm">{{'smd.defence.black.ip.unlock'|trans}}</a>

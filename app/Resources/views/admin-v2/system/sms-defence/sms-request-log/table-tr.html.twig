<tr id="sms-request-log-table-tr-{{ smsRequestLog.id }}">
  <td>
    <span>{{ smsRequestLog.coordinate }}</span>
  </td>
  <td>
    <span>{{ smsRequestLog.ip }}</span>
  </td>
  <td>
    <span>{% include 'user-mobile-mask.html.twig' with { mobile: smsRequestLog.mobile } %}</span>
  </td>
  <td>
    {% if smsRequestLog.isIllegal == 0 %}
      <span>{{ 'sms.defence.request.log.is_illegal.no'|trans }}</span>
    {% else %}
      <span style="color: red">{{ 'sms.defence.request.log.is_illegal.yes'|trans }}</span>
    {% endif %}
  </td>
  <td>
    {% if smsRequestLog.disableType == 'ip' %}
      <span>{{ 'sms.defence.request.log.disable_type.ip'|trans }}</span>
    {% elseif smsRequestLog.disableType == 'coordinate' %}
      <span>{{ 'sms.defence.request.log.disable_type.coordinate'|trans }}</span>
    {% else %}
      <span>{{ 'sms.defence.request.log.disable_type.none'|trans }}</span>
    {% endif %}
  </td>
  <td>
    <span class="text-sm">{{ smsRequestLog.createdTime|date('Y-n-d H:i:s') }}</span>
  </td>
  <td>
    <a href="#modal" data-toggle="modal" data-url="{{ path('admin_v2_sms_request_log_detail', {id:smsRequestLog.id}) }}"
       data-url="" class="btn btn-default btn-sm">{{ 'admin.user.btn.show_btn'|trans }}</a>
  </td>
</tr>
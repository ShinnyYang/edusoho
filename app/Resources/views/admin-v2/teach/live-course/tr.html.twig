<tr id="tag-tr-{{ liveTask.id }}">
  {% set lessonUrl = path('course_task_show', {courseId:liveTask.courseId, id:liveTask.id}) %}
  <td>
    <a href="{{ lessonUrl }}" target="_blank" class="font-bold">{{ liveTask.title }}</a>
  </td>
  <td>
    <div class="mts">
      {% if courseSet %}
        <a class="" href="{{ path('course_show',{id: courseSet.defaultCourseId}) }}" target="_blank">{{ courseSet.title }}</a>
      {% else %}
        <span class="text-muted">{{'admin.live_manage.deleted'|trans}}</span>
      {% endif %}
    </div>
  </td>
  <td>
    {% if liveTask.classroom %}
      <a class="" href="{{ path('classroom_show',{id: liveTask.classroom.id}) }}" target="_blank">{{ liveTask.classroom.title }}</a>

    {% else %}
      -
    {% endif %}
  </td>
  {% include 'org/parts/table-body-td.html.twig' with {orgCode: courseSet.orgCode}%}
  <td>{{ liveTask.startTime|date('Y-m-d H:i') }}</td>
  <td>{{ liveTask.length }}</td>
  <td>
    {% if courseSet %}
      {{ courseSet.maxStudentNum }}
    {% else %}
      <span class="text-muted">{{'admin.live_manage.deleted'|trans}}</span>
    {% endif %}
  </td>
  {% set url = path('course_set_manage_live_statistics', {courseSetId: courseSet.id, courseId: liveTask.courseId}) %}
  <td><a href="{{ url }}#/details?taskId={{ liveTask.id }}">{{ 'site.btn.detail'|trans }}</a></td>
</tr>
<div class="admin-sidebar-section__header">{{ 'admin_v2.default_page.changelog_title'|trans }}<a class="admin-sidebar-section__more" href="{{ path('changelog_list') }}" target="_blank">{{ 'site.datagrid.filter.category_more'|trans }}<i class="es-icon es-icon-chevronright"></i></a></div>
<div class="admin-sidebar-section__body">
  <div class="admin-log">
    {% for item in currentChangelog.items %}
      {% if loop.index == 1 %}
      <div class="admin-log__cell text-overflow" style="display: inline;">
        <span class="admin-log__date cd-mr16" style="border: #999999 1px solid;padding:1px;">{{ item }}</span>
      </div>
      {% else %}
        <div class="admin-log__cell text-overflow" {% if loop.index ==2 %} style="display: inline;" {% endif %}>
          <span class="admin-log__date cd-mr16">{{ item }}</span>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</div>


{% extends 'course-manage/layout.html.twig' %}

{% block title %}{{'学员错题'|trans}} - {{ course.title }} - {{ parent() }}{% endblock %}
{% set course_side_nav = 'wrong-questions' %}
{% block main_heading_class %}course-main-header{% endblock %}

{% do script(['app/vue/views/course_manage/wrong_question/index.js']) %}
{% do css(['app/css/main-ant.css']) %}

{% block main_heading %}
  {% include 'course-manage/panel-header/course-publish-header.html.twig' with {code: course_side_nav,btnGroup:0} %}
{% endblock %}

{% block main_body %}
  <input type="hidden" class="js-course-id" value="{{ course.id }}" />
  <input type="hidden" class="js-course-set-id" value="{{ courseSet.id }}" />
  <div id="app">
  </div>
{% endblock %}

{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "ant-design-vue",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "common": "../app/Resources/static-src/common",
          "app": "../app/Resources/static-src/app"
        }
      }
    ]
  ]
}
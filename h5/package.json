{
  "name": "edusoho-cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev:h5": "vue-cli-service serve --mode  h5",
    "dev:admin": "vue-cli-service serve --mode  admin",
    "build:h5": "vue-cli-service build --mode prod h5",
    "build:admin": "vue-cli-service build --mode prod admin",
    "build": "vue-cli-service build --mode prod h5 && vue-cli-service build --mode prod admin",
    "analyze:h5": "vue-cli-service build --mode prod h5 --report",
    "analyze:admin": "vue-cli-service build --mode prod admin --report",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint  --ext .js --ext .jsx --ext .vue src/",
    "lint:fix": "eslint  --fix  --ext .js --ext .jsx --ext .vue src/",
    "style-lint": "stylelint  src/**/*.scss",
    "style-lint:fix": "stylelint src/**/*.scss --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --syntax=scss --fix"
    ],
    "*.{js,vue}": [
      "eslint  --fix --ext .js --ext .jsx --ext .vue",
      "prettier --write",
      "git add"
    ]
  },
